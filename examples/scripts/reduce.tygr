type mylist = list

let rec fold_right f, ls, x => match ls with
    | [] => x
    | hd::tl => f hd (fold_right f tl x)

let result_b = fold_right (fn x, acc => if x then acc else !acc) (true::false::[]) true

let fold_left = rec self => fn f => fn acc => fn ls => match ls with
    | [] => acc
    | hd::tl => self f (f acc hd) tl

let result_l = fold_left (fn acc => fn x => acc + x) 0 (3::5::[])
